{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "public/src/App.js", "public/src/App.js"],
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "strict":false,
    "noImplicitAny": false,
  },

  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
